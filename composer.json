{
    "name": "league/EvangelistStatus",
    "description": "An agnostic package that grades Github users based on the number of open source projects they own",
    "keywords": [
        "league",
        "EvangelistStatus"
    ],
    "homepage": "https://github.com/andela-ooduye/EvangelistStatus",
    "license": "MIT",
    "authors": [
        {
            "name": "Oduye Oluwayemisi",
            "email": "oduye.oluwayemisi@andela.com",
            "role": "Developer",
            "homepage": "http://oluwayemisioduye.com"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "League\\EvangelistStatus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\EvangelistStatus\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
